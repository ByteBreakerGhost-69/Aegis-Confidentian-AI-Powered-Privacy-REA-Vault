# .env.example - Copy ke .env dan isi

# ========== PRIVATE KEY ==========
# Dapatkan dari MetaMask (jangan pernah share!)
PRIVATE_KEY=your_private_key_here

# ========== RPC URLs ==========
# Sepolia - Dapatkan dari Infura/Alchemy
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_project_id
# Mumbai - Dapatkan dari Alchemy
MUMBAI_RPC_URL=https://polygon-mumbai.g.alchemy.com/v2/your_api_key

# ========== API KEYS untuk VERIFICATION ==========
ETHERSCAN_API_KEY=your_etherscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# ========== API KEYS untuk AI (Chainlink Functions) ==========
OPENAI_API_KEY=sk-your_openai_api_key
GEMINI_API_KEY=AIza_your_gemini_api_key

# ========== GAS REPORTER (optional) ==========
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key
REPORT_GAS=false

# ========== CHAINLINK FUNCTIONS ==========
# Subscription ID setelah setup
FUNCTIONS_SUBSCRIPTION_ID=1234
